[run]
branch = True
source = .
omit =
    */site-packages/*
    test_*.py
    *_test.py
    whitebox_testing.py
    generate_test_dashboard.py
    cv2.py

[report]
exclude_lines =
    pragma: no cover
include =
    config.py
    utils.py
    feature_detection.py
    visualization.py
    ela_analysis.py
    advanced_analysis.py
    app2.py
    classification.py
    copy_move_detection.py
    export_utils.py
    jpeg_analysis.py
    main.py
    validation.py

show_missing = true
sort = Cover